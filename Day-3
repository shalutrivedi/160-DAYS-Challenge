class Solution {
  public:
    int minRemoval(vector<vector<int>> &intervals) {
        // code here
        sort(intervals.begin(),intervals.end(),[](const vector<int> & a,const vector<int> &b){
            return a[1]<b[1];
        });
        int prev_end=INT_MIN;
        int remove_count=0;
        for(const auto & interval:intervals){
            if(interval[0]<prev_end){
                remove_count++;
            }
            else{
                prev_end=interval[1];
            }
        }
        return remove_count;
    }
};


